MOD_A: BEGIN

    PUBLIC PONTO_FINAL
    PUBLIC NUM1, NUM2, NUM3 

    EXTERN INICIO_C

    INPUT NUM1      
    INPUT NUM1+1    
    INPUT NUM2      
    INPUT NUM2+1    
    INPUT NUM3      
    INPUT NUM3+1    

    JMP INICIO_C    

PONTO_FINAL:
    STOP            

    NUM1: SPACE 2   
    NUM2: SPACE 2   
    NUM3: SPACE 2   
END
