
N:          CONST 8     
FIB_N_1:    CONST 1     
FIB_N_2:    CONST 0     
RESULTADO:  SPACE       
UM:         CONST 1     

LOOP:
    LOAD N
    JMPZ FIM

    LOAD FIB_N_1
    ADD FIB_N_2
    STORE RESULTADO

    COPY FIB_N_1, FIB_N_2
    COPY RESULTADO, FIB_N_1

    LOAD N
    SUB UM
    STORE N

    JMP LOOP

FIM:
    STOP
